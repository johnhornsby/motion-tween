  },
  "babel": {
    "presets" : ["es2015", "stage-0"]
  },
  "author": "john@interactivelabs.co.uk",
  "license": "ISC",
  "devDependencies": {
  	"babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-jest": "^12.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "jest-cli": "^12.1.0",
    "webpack": "^1.12.1"
  }
}
